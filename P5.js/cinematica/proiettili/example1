let r;
let x;
let y;

let vx;
let vy;

let ax;
let ay;

function setup() {
  createCanvas(600, 400);
  background(225);

  r = 100;
  x = 100;
  y = 100;
  
  vx = 5;
  vy = 1;
  
  ax = 0;
  ay = 0.1;
  
}

function draw() {
  background(225);

  stroke(100,100,100);
  fill(225);
  strokeWeight(4);
  
  vx = vx + ax;
  vy = vy + ay;
  
  x = x + vx;
  y = y + vy;
  
  circle(x, y, r); 
  
}
